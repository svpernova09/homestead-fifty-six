        "symfony/yaml": "~3.0||~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 || ^6.0",
        "phly/changelog-generator": "^2.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylorotwell@gmail.com"
        },
        {
            "name": "Joe Ferguson",
            "email": "joe@joeferguson.me"
        }
